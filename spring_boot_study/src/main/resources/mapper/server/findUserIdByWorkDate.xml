<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="CJ.CJ.mapper.server.FindUserIdByWorkDateMapper">
    <select id="findUserInfoByWorkDate" parameterType="hashmap" resultType="java.lang.String">
        SELECT
        userId
        FROM HeartRateHistory
        WHERE
        workDate BETWEEN #{startWorkDate} AND #{endWorkDate}
    </select>
</mapper>